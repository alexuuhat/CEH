#Get all users
impacket-GetADUsers -all -dc-ip 10.10.11.173 support.htb/ldap:'nvEfEK16^1aM4$e7AclUf8x$tRWxPWO1%lmz'
#Add an SPN to maria's account as bloodhound suggested
